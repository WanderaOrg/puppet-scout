account_key: <%= @account_key %>
hostname: <%= @hostname || '' %>
display_name: <%= @display_name || '' %>
log_file: <%= @log_file || '' %>
ruby_path: <%= @ruby_path || '' %>
environment: <%= @environment || '' %>
<% if @roles -%>
roles: <%= @roles.join(',') %>
<% end -%>
http_proxy: <%= @http_proxy || '' %>
https_proxy: <%= @https_proxy || '' %>
statsd:
  addr: <%= @statsd_address || '127.0.0.1:8125' %>
  enabled: <%= @statsd_enabled %>
